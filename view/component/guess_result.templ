package component

import (
    "yugiohdle/model"
    "strconv"
)

templ GuessRow(guess model.Guess) {
    <tr>
        <td>{ guess.Name }</td>
        <td>{ guess.Type }</td>
        <td>{ strconv.Itoa(guess.Atk) }</td>
        <td>{ strconv.Itoa(guess.Def) }</td>
        <td>{ strconv.Itoa(guess.Level) }</td>
        <td>{ guess.Race }</td>
        <td>{ guess.Attribute }</td>
    </tr>
}
