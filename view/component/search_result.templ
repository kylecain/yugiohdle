package component

import (
    "yugiohdle/model"
)

templ SearchResult(cards []model.Card) {
    for _, card := range cards {
        <div
            x-on:click="
                $refs.searchBar.value = $el.textContent;
                showSearchResults = false
            "
            style="cursor: pointer;"
        >
            { card.Name }
        </div>
    }
}
